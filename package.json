  "author": "Jean-Christophe Hoelt <hoelt@fovea.cc>",
  "license": "GPL",
  "dependencies": {
    "alkindi": "^1.1.1",
    "authdb": "^0.1.0",
    "binary-search-tree-continued": "0.0.4",
    "blanket": "^1.1.6",
    "bunyan": "^1.2.3",
    "coffeelint": "^1.6.1",
    "coffeescript": "^2.3.1",
    "concat-stream": "^1.4.7",
    "control.async": "^0.5.2",
    "data.maybe": "^1.2.1",
    "data.task": "^3.0.0",
    "eslint": "^0.10.0",
    "forever": "^0.13.0",
    "ganomede-tagizer": "^2.0.1",
    "jshint": "^2.5.10",
    "mocha": "^5.2.0",
    "redis": "^0.12.1",
    "restify": "^4.2.0",
    "vasync": "^1.6.2"
  },
  "config": {
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "fakeredis": "^0.3.1",
    "ganomede-substract-game": "^1.1.0",
